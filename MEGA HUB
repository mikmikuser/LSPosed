-- Load Orion Library
local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source'))()

-- Create Main Window
local Window = OrionLib:MakeWindow({
    Name = "MEGA HUB",
    HidePremium = false,
    IntroText = "MEGA HUB V1",
    SaveConfig = true,
    ConfigFolder = "MEGA HUBV1"
})

-- Notifications
OrionLib:MakeNotification({
    Name = "THANK YOU FOR USING MY SCRIPT",
    Content = "GAME SUPPORTED!!!",
    Image = "rbxassetid://4483345998",
    Time = 10
})

OrionLib:MakeNotification({
    Name = "COPIED!!!",
    Content = "https://discord.com/invite/eejZ9Hh8SJ",
    Image = "rbxassetid://4483345998",
    Time = 10
})

-- Copy link to clipboard
setclipboard("https://discord.com/invite/eejZ9Hh8SJ")

-- Main Tab
local MainTab = Window:MakeTab({
    Name = "MAIN",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Main Section
local Section = MainTab:AddSection({
    Name = "☪ Main ☪"
})

-- Add Buttons to Main Tab
MainTab:AddButton({
    Name = "MATSUNE HUB V3",
    Callback = function()
        local success, err = pcall(function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Yatsuraa/Matsune/main/MatsuneV3.lua"))()
        end)
        if not success then
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "Failed to load Matsune HUB V3: " .. err,
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end    
})

MainTab:AddButton({
    Name = "HOHO HUB",
    Callback = function()
        local success, err = pcall(function()
            loadstring(game:HttpGet('https://raw.githubusercontent.com/acsu123/HohoV2/Free/BloxFruitFreeV3.lua'))()
        end)
        if not success then
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "Failed to load HOHO HUB: " .. err,
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end    
})

